{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": ["w3c-image-capture"]
  },
  "files": ["src/main.ts", "src/polyfills.ts"],
  "include": ["src/**/*.ts", "src/**/*.d.ts"],
  "exclude": [
    "src/**/*.spec.ts",
    "src/**/*-testing.*.ts",
    "src/**/mock-*.ts",
    "src/test.ts",
    "src/environments/environment.prod.ts"
  ]
}
